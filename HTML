<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Debugging Demo</title>
  <style>
    #box {
      width: 100px;
      height: 100px;
      background-color: lightblue;
      margin-top: 20px;
    }
  </style>
</head>
<body>
  <h1>Debugging Demo Page</h1>
  <button onclick="moveBox()">Move Box</button>
  <div id="box"></div>

  <script>
    let position = 0;

    function moveBox() {
      console.log("Moving box...");

      position += 50;
      const box = document.getElementById("box");

      // Introduce a bug: typo in 'style'
      box.styl.left = position + "px";

      console.log("New position:", position);
    }
  </script>
</body>
</html>


